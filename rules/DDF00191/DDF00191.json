{
    "rule": {
        "Authorities": [
            {
                "Organization": "CDISC",
                "Standards": [
                    {
                        "Name": "USDM",
                        "References": [
                            {
                                "Citations": [
                                    {
                                        "Document": "USDM_CORE_Rules.xlsx",
                                        "Cited_Guidance": "A masking is not expected to be defined for any study role in a study design with an open label blinding schema."
                                    }
                                ],
                                "Origin": "USDM Conformance Rules",
                                "Version": "1.0",
                                "Rule_Identifier": {
                                    "Id": "DDF00191",
                                    "Version": "1"
                                }
                            }
                        ],
                        "Version": "4.0"
                    }
                ]
            }
        ],
        "Check": "$.study.versions@$sv.\n  [\n    ($sv.roles)@$r.($sv.studyDesigns)@$sd\n      [\n        (\n            $sv.id in $r.appliesToIds or\n            $sd.id in $r.appliesToIds\n        ) and\n        $r.masking.isMasked and\n        $sd.blindingSchema.standardCode.code = \"C49659\"\n      ].{\n          \"instanceType\": $r.instanceType,\n          \"id\": $r.id,\n          \"path\": $r._path,\n          \"name\": $r.name,\n          \"code\": $r.code.decode,\n          \"masking.text\": $r.masking.text,\n          \"masking.isMasked\": $r.masking.isMasked,\n          \"appliesToIds\": $r.appliesToIds,\n          \"StudyDesign.id\": $sd.id,\n          \"StudyDesign.name\": $sd.name,\n          \"StudyDesign.blindingSchema\": $sd.blindingSchema.standardCode.decode\n        }\n  ]",
        "Core": {
            "Id": "",
            "Status": "Draft",
            "Version": "1"
        },
        "Description": "A masking is not expected to be defined for any study role in a study design with an open label blinding schema.",
        "Executability": "Partially Executable - Possible Overreporting",
        "Outcome": {
            "Message": "A masking is defined for the study role, but the role applies to a study design with an open label blinding schema.",
            "Output_Variables": [
                "name",
                "code",
                "masking.text",
                "masking.isMasked",
                "appliesToIds",
                "StudyDesign.id",
                "StudyDesign.blindingSchema"
            ]
        },
        "Scope": {
            "Entities": {
                "Include": [
                    "StudyRole"
                ]
            }
        },
        "Sensitivity": "Record",
        "Rule_Type": "JSONata"
    },
    "datasets": [],
    "codelists": []
}