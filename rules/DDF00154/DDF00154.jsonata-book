[{"kind":1,"language":"markdown","value":"Import utility functions","outputs":[]},{"kind":2,"language":"jsonata","value":"$utils := $merge([$import(\"./utils/get_idmap.jsonata\")\r\n                 ,$import(\"./utils/get_maxids.jsonata\")\r\n                 ,$import(\"./utils/make_dup.jsonata\")\r\n                 ,$import(\"./utils/get_rule.jsonata\")\r\n                 ,$import(\"./utils/report.jsonata\")]) ","outputs":[]},{"kind":1,"language":"markdown","value":"Define test","outputs":[]},{"kind":2,"language":"jsonata","value":"$rule := $utils.get_rule($import(\"./rules/DDF00154/DDF00154.json\"))","outputs":[]},{"kind":1,"language":"markdown","value":"Create test data","outputs":[]},{"kind":2,"language":"jsonata","value":"(\r\n    /***********************************************************************\\\r\n    * Use the CDISC Pilot data as a start point and add BC categories        *\r\n    \\***********************************************************************/\r\n\r\n    $old_study := $loadFile(\"./testdata/CDISC_Pilot_Study.json\")\r\n    ~> |study.versions[0].studyDesigns.characteristics|\r\n        {\r\n            \"code\":code=\"C99907x1\"?\"C207613\":code,\r\n            \"decode\": $substring(decode,0,1)&$lowercase($substring(decode,1))\r\n        }|;\r\n\r\n    $makechar:=function($study,$code,$decode){\r\n        (\r\n            $maxids := $utils.get_maxids($study);\r\n            $idmap := $utils.get_idmap($study.study.versions[0].studyDesigns[0].characteristics[0],$maxids);\r\n            $new_char := $utils.make_dup($study.study.versions[0].studyDesigns[0].characteristics[0],$idmap)\r\n                ~> |$|  {\r\n                            \"code\": $code,\r\n                            \"decode\": $decode\r\n                        }|;\r\n        )\r\n    };\r\n\r\n    /***********************************************************************\\\r\n    * Create and insert a copy of the first study design as an              *\r\n    * observational study design.                                           *\r\n    \\***********************************************************************/\r\n    $maxids := $utils.get_maxids($old_study);\r\n    $idmap2 := $utils.get_idmap($old_study.study.versions[0].studyDesigns[0],$maxids);\r\n    $new_sd2 := $utils.make_dup($old_study.study.versions[0].studyDesigns[0],$idmap2)\r\n                ~> |$|{\r\n                        \"id\": \"ObservationalStudyDesign_1\",\r\n                        \"instanceType\": \"ObservationalStudyDesign\",\r\n                        \"name\": \"Study Design 4\",\r\n                        \"description\": \"An observational design for the study\",\r\n                        \"timePerspective\": (blindingSchema.standardCode ~> |$|{\"code\": \"C15273\",\"decode\": \"PROSPECTIVE\"}|)\r\n                      },[\"blindingSchema\",\"intentTypes\",\"subTypes\"]|\r\n                ~> |studyType|{\"code\": \"C16084\", \"decode\": \"OBSERVATIONAL\"}|\r\n                ~> |model|{\"code\": \"C15208\", \"decode\": \"COHORT\"}|\r\n                ~> |characteristics[0]|  {\r\n                            \"code\": \"C217004\",\r\n                            \"decode\": \"Single-Centre\"\r\n                        }|\r\n                ~> |characteristics[1]|  {\r\n                            \"code\": \"C217006\",\r\n                            \"decode\": \"Single Country\"\r\n                        }|;\r\n    $new_study2 := $old_study\r\n                ~> |study.versions[0]|{\"studyDesigns\": $append(studyDesigns,$new_sd2)}|;\r\n\r\n    $new_study2 := $new_study2 ~> |study.versions[0].studyDesigns[0]|{\"characteristics\":$append(characteristics, $makechar($new_study2,\"C217005\",\"Multicentre\"))}|;\r\n    $new_study2 := $new_study2 ~> |study.versions[0].studyDesigns[0]|{\"characteristics\":$append(characteristics, $makechar($new_study2,\"C217007\",\"Multiple Countries\"))}|;\r\n\r\n    /***********************************************************************\\\r\n    * Write out as positive unit test data.                                 *\r\n    \\***********************************************************************/\r\n    $writeFile(\"./rules/DDF00154/unit-test-coreid-DDF00154-positive-1.json\",$new_study2);\r\n    \r\n    /***********************************************************************\\\r\n    * Make the following updates then write out as negative unit test data: *\r\n    * - Insert a new characteristic for the first study design as a copy of *\r\n    *   the first characteristic, but using NCI preferred terms as decode.  *\r\n    * - Apply the same code for the first 3 characteristics of the second   *\r\n    *   study design.                                                       *\r\n    * - Duplicate the codes for the first/second and third/fourth           *\r\n    *   characteristics for the third study design.                         *\r\n    \\***********************************************************************/\r\n    $new_study3 := $new_study2 ~> |study.versions[0].studyDesigns[0]|{\"characteristics\":$append(characteristics, $makechar($new_study2,\"C217004\",\"Single-Center Study\"))}|;\r\n    $new_study3 := $new_study3 ~> |study.versions[0].studyDesigns[0]|{\"characteristics\":$append(characteristics, $makechar($new_study3,\"C217006\",\"Single Country\"))}|;\r\n    $new_study3 := $new_study3 ~> |study.versions[0].studyDesigns[1]|{\"characteristics\":$append(characteristics, $makechar($new_study2,\"C217005\",\"Multicenter Study\"))}|;\r\n    $new_study3 := $new_study3 ~> |study.versions[0].studyDesigns[1]|{\"characteristics\":$append(characteristics, $makechar($new_study3,\"C217007\",\"Multiple Countries\"))}|;\r\n\r\n    $writeFile(\"./rules/DDF00154/unit-test-coreid-DDF00154-negative-1.json\",$new_study3);\r\n    $list:=function($study){\r\n          $study.study.versions.studyDesigns{id:characteristics.(\"** code=\"&code&\", decode='\"&decode&\"'\")} ~>\r\n        $each(function($v,$k){\"* \"&$k&\"\\n\"&$join($v,\"\\n\")})~>$join(\"\\n\")\r\n    };\r\n    \"Positive:\\n\"&$list($new_study2)&\"\\nNegative:\\n\"&$list($new_study3)\r\n)","outputs":[]},{"kind":1,"language":"markdown","value":"Test positive dataset","outputs":[]},{"kind":2,"language":"jsonata","value":"(\r\n    $pos_data := $loadFile(\"./rules/DDF00154/unit-test-coreid-DDF00154-positive-1.json\");\r\n    $utils.report($rule,$pos_data)\r\n)","outputs":[]},{"kind":1,"language":"markdown","value":"Test negative data","outputs":[]},{"kind":2,"language":"jsonata","value":"(\r\n    $neg_data := $loadFile(\"./rules/DDF00154/unit-test-coreid-DDF00154-negative-1.json\");\r\n    $utils.report($rule,$neg_data)\r\n)","outputs":[]}]