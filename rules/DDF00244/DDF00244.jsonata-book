[{"kind":1,"language":"markdown","value":"Import utility functions","outputs":[]},{"kind":2,"language":"jsonata","value":"$utils := $merge([$import(\"./utils/get_idmap.jsonata\")\r\n                 ,$import(\"./utils/get_maxids.jsonata\")\r\n                 ,$import(\"./utils/make_dup.jsonata\")\r\n                 ,$import(\"./utils/get_rule.jsonata\")\r\n                 ,$import(\"./utils/get_ref_value.jsonata\")\r\n                 ,$import(\"./utils/report.jsonata\")]) ","outputs":[]},{"kind":1,"language":"markdown","value":"Define test","outputs":[]},{"kind":2,"language":"jsonata","value":"$rule := $utils.get_rule($import(\"./rules/DDF00244/DDF00244.json\"))","outputs":[]},{"kind":1,"language":"markdown","value":"Create test data","outputs":[]},{"kind":2,"language":"jsonata","value":"(\r\n    /***********************************************************************\\\r\n    * Use the CDISC Pilot data as a start point                             *\r\n    \\***********************************************************************/\r\n\r\n    $old_study :=   $loadFile(\"./testdata/CDISC_Pilot_Study.json\");\r\n\r\n    /***********************************************************************\\\r\n    * Write out as positive unit test data.                                 *\r\n    \\***********************************************************************/\r\n    $writeFile(\"./rules/DDF00244/unit-test-coreid-DDF00244-positive.json\",$old_study);\r\n    \r\n    /***********************************************************************\\\r\n    * Add the following invalid references and then write out as negative   *\r\n    * unit test data:                                                       *\r\n    * - A partial usdm:ref where the klass specification is missing         *\r\n    * - An invalid usdm:ref where the referenced id value does not exist    *\r\n    \\***********************************************************************/\r\n    $new_study3 :=  $old_study\r\n                    ~> |study.versions[0].narrativeContentItems[id=\"NarrativeContentItem_1\"]|\r\n                        {\r\n                            \"text\": $replace(\r\n                                        text,\r\n                                        /<usdm:ref attribute=\\\"text\\\" id=\\\"StudyIdentifier_1\\\" klass=\\\"StudyIdentifier\\\"><\\/usdm:ref>/,\r\n                                        \"<usdm:ref attribute=\\\"text\\\" id=\\\"StudyIdentifier_1\\\"></usdm:ref>\"\r\n                                    )\r\n                        }|\r\n                    ~> |study.versions[0].narrativeContentItems[id=\"NarrativeContentItem_15\"]|\r\n                        {\r\n                            \"text\": $replace(\r\n                                        text,\r\n                                        /EligibilityCriterionItem_12/,\r\n                                        \"EligibilityCriterionItem_xx\")\r\n                        }|;\r\n    $writeFile(\"./rules/DDF00244/unit-test-coreid-DDF00244-negative.json\",$new_study3);\r\n)","outputs":[]},{"kind":1,"language":"markdown","value":"Test positive data","outputs":[]},{"kind":2,"language":"jsonata","value":"(\r\n    $pos_data := $loadFile(\"./rules/DDF00244/unit-test-coreid-DDF00244-positive.json\");\r\n    $utils.report($rule,$pos_data);\r\n)","outputs":[]},{"kind":1,"language":"markdown","value":"Test negative data","outputs":[]},{"kind":2,"language":"jsonata","value":"(\r\n    $neg_data := $loadFile(\"./rules/DDF00244/unit-test-coreid-DDF00244-negative.json\");\r\n    $utils.report($rule,$neg_data)\r\n)","outputs":[]}]