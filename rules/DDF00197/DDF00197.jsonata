{
    "check_func" : function($data){$data.(
  study.versions.studyDesigns.
    {
      "instanceType": instanceType,
      "id": id,
      "path": _path,
      "name": name,
      "Duplicate documentVersionIds":
        $filter(documentVersionIds,function($v,$i,$a){$count($a[$=$v])>1}) ~> $sort
    }[`Duplicate documentVersionIds`]
                )},
    "message": "The study design is documented more than once by the same study definition document version.",
    "variables": [
            "name",
            "Duplicate documentVersionIds"
        ],
    "scope": {
        "Include": [
            "InterventionalStudyDesign",
            "ObservationalStudyDesign"
        ]
    }
}