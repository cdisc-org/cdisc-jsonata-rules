{
    "rule": {
        "Authorities": [
            {
                "Organization": "CDISC",
                "Standards": [
                    {
                        "Name": "USDM",
                        "References": [
                            {
                                "Citations": [
                                    {
                                        "Document": "USDM_CORE_Rules.xlsx",
                                        "Cited_Guidance": "There must be exactly one sponsor study identifier (i.e., a study identifier whose scope is an organization that is identified as the organization for the sponsor study role)."
                                    }
                                ],
                                "Origin": "USDM Conformance Rules",
                                "Version": "1.0",
                                "Rule_Identifier": {
                                    "Id": "DDF00172",
                                    "Version": "1"
                                }
                            }
                        ],
                        "Version": "4.0"
                    }
                ]
            }
        ],
        "Check": "[$.study.versions@$sv.\n    (\n        $spIds := $sv.studyIdentifiers[scopeId in $sv.roles[code.code=\"C70793\"].organizationIds].text;\n        {\n            \"id\": $sv.id,\n            \"versionIdentifier\": $sv.versionIdentifier,\n            \"dataset\": $sv.instanceType,\n            \"Sponsor Identifier(s)\": $spIds,\n            \"count\": $count($spIds)\n        }\n    )][count != 1]",
        "Core": {
            "Id": "",
            "Status": "Draft",
            "Version": "1"
        },
        "Description": "There must be exactly one sponsor study identifier (i.e., a study identifier whose scope is an organization that is identified as the organization for the sponsor study role).",
        "Executability": "Fully Executable",
        "Outcome": {
            "Message": "The study version does not have exactly one sponsor study identifier.",
            "Output_Variables": [
                "id",
                "versionIdentifier",
                "Sponsor Identifier(s)",
                "count"
            ]
        },
        "Scope": {
            "Entities": {
                "Include": [
                    "StudyVersion"
                ]
            }
        },
        "Sensitivity": "Record",
        "Rule_Type": "Record Data"
    },
