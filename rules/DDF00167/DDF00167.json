        "Check": "study.versions.{\n        \"id\": id,\n        \"versionIdentifier\": versionIdentifier,\n        \"Duplicate documentVersionIds\": $filter(documentVersionIds,function($v,$i,$a){$count($a[$=$v])>1}) ~> $sort,\n        \"dataset\": instanceType\n    }[`Duplicate documentVersionIds`]",
