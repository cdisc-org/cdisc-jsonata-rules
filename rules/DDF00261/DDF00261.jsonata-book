[{"kind":1,"language":"markdown","value":"Import utility functions","outputs":[]},{"kind":2,"language":"jsonata","value":"$utils := $merge([$import(\"./utils/get_idmap.jsonata\")\r\n                 ,$import(\"./utils/get_maxids.jsonata\")\r\n                 ,$import(\"./utils/make_dup.jsonata\")\r\n                 ,$import(\"./utils/get_rule.jsonata\")\r\n                 ,$import(\"./utils/report.jsonata\")]) ","outputs":[]},{"kind":1,"language":"markdown","value":"Define test","outputs":[]},{"kind":2,"language":"jsonata","value":"$rule := $utils.get_rule($import(\"./rules/DDF00261/DDF00261.json\"))","outputs":[]},{"kind":1,"language":"markdown","value":"Create test data","outputs":[]},{"kind":2,"language":"jsonata","value":"(\r\n    /***********************************************************************\\\r\n    * Use the CDISC Pilot data as a start point                             *\r\n    \\***********************************************************************/\r\n\r\n    $old_study :=   $loadFile(\"./testdata/CDISC_Pilot_Study.json\")\r\n                    ~> |study.documentedBy.versions.dateValues.type[code=\"C99903x1\"]|{\"code\": \"C71476\",\"decode\":\"Approval Date\"}|;\r\n\r\n    $writeFile(\"./rules/DDF00261/unit-test-coreid-DDF00261-positive-1.json\",$old_study);\r\n\r\n    /***********************************************************************\\\r\n    * Create and insert a new version for the first study definition        *\r\n    * document version.                                    *\r\n    \\***********************************************************************/\r\n\r\n    $new_study2 :=  $old_study\r\n                    ~> |study.versions[0].dateValues[0].geographicScopes[0].type|{\"code\": \"C41129\", \"decode\": \"Region\"}|\r\n                    ~> |study.versions[0].amendments[0].geographicScopes[0].type|{\"code\": \"C25464\", \"decode\": \"Country\"}|\r\n                    ~> |study.versions[0].amendments[0].enrollments[0].forGeographicScope.type|{\"code\": \"C68846\", \"decode\": \"Global\"}|\r\n                    ~> |study.versions[0].amendments[0].dateValues[0].geographicScopes[0].type|{\"code\": \"C25464\", \"decode\": \"Country\"}|\r\n                    ~> |study.documentedBy[1].versions[0].dateValues[0].geographicScopes[0]|{},[\"code\"]|;\r\n\r\n    $writeFile(\"./rules/DDF00261/unit-test-coreid-DDF00261-negative-1.json\",$new_study2);\r\n    $rep:=function($study){\r\n      $study.study@$s.\r\n    $s.documentedBy@$sdd.\r\n      $sdd.versions@$sddv.{\"* \"&$sddv.id:\r\n       $sddv.dateValues@$dv.\r\n        $dv.geographicScopes\r\n      {\"** \"&$dv.(type.decode&\"\\n*** \"&id):$join($.(\"**** \"&id & \": \" & type.decode & (code?\" (\" & code.standardCode.decode & \")\")),\"\\n\")}\r\n      ~>$each(function($v,$k){$k&\"\\n\"&$v}) ~> $join(\"\\n\")}\r\n      ~> $merge()~>$each(function($v,$k){$k&\":\\n\"&$v}) ~> $join(\"\\n\")\r\n    };\r\n    \"Positive:\\n\" & $rep($new_study1) & \"\\n\\nNegative:\\n\" & $rep($new_study2)\r\n)","outputs":[]},{"kind":1,"language":"markdown","value":"Test positive data","outputs":[]},{"kind":2,"language":"jsonata","value":"(\r\n    $pos_data := $loadFile(\"./rules/DDF00261/unit-test-coreid-DDF00261-positive-1.json\");\r\n    $utils.report($rule,$pos_data);\r\n)","outputs":[]},{"kind":1,"language":"markdown","value":"Test negative data","outputs":[]},{"kind":2,"language":"jsonata","value":"(\r\n    $neg_data := $loadFile(\"./rules/DDF00261/unit-test-coreid-DDF00261-negative-1.json\");\r\n    $utils.report($rule,$neg_data)\r\n)","outputs":[]}]