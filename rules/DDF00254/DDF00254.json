{
    "rule": {
        "Authorities": [
            {
                "Organization": "CDISC",
                "Standards": [
                    {
                        "Name": "USDM",
                        "References": [
                            {
                                "Citations": [
                                    {
                                        "Document": "USDM_CORE_Rules.xlsx",
                                        "Cited_Guidance": "An activity must only reference child activities that are specified within the same study design."
                                    }
                                ],
                                "Origin": "USDM Conformance Rules",
                                "Version": "1.0",
                                "Rule_Identifier": {
                                    "Id": "DDF00254",
                                    "Version": "1"
                                }
                            }
                        ],
                        "Version": "4.0"
                    }
                ]
            }
        ],
        "Check": "($.**.studyDesigns)@$s.\n    [\n        $s.activities.childIds[$not($ in $s.activities.id)].\n        {\n        \"StudyDesign.id\": $s.id,\n        \"StudyDesign.name\": $s.name,\n        \"id\": %.id,\n        \"name\": %.name,\n        \"childId\": $,\n        \"instanceType\": %.instanceType\n        }\n    ]",
        "Core": {
            "Id": "",
            "Status": "Draft",
            "Version": "1"
        },
        "Description": "An activity must only reference child activities that are specified within the same study design.",
        "Executability": "Fully Executable",
        "Outcome": {
            "Message": "The activity references a childId that does not match the id of any activity defined within the same study design as the activity.",
            "Output_Variables": [
                "StudyDesign.id",
                "StudyDesign.name",
                "id",
                "name",
                "childId"
            ]
        },
        "Scope": {
            "Entities": {
                "Include": [
                    "Activity"
                ]
            }
        },
        "Sensitivity": "Record",
        "Rule_Type": "Record Data"
    },
    "datasets": [],
    "codelists": []
}