[{"kind":1,"language":"markdown","value":"Import utility functions","outputs":[]},{"kind":2,"language":"jsonata","value":"$utils := $merge([$import(\"./utils/get_idmap.jsonata\")\r\n                 ,$import(\"./utils/get_maxids.jsonata\")\r\n                 ,$import(\"./utils/make_dup.jsonata\")\r\n                 ,$import(\"./utils/get_rule.jsonata\")\r\n                 ,$import(\"./utils/report.jsonata\")]) ","outputs":[]},{"kind":1,"language":"markdown","value":"Define test","outputs":[]},{"kind":2,"language":"jsonata","value":"$rule := $utils.get_rule($import(\"./rules/DDF00192/DDF00192.json\"))","outputs":[]},{"kind":1,"language":"markdown","value":"Create test data","outputs":[]},{"kind":2,"language":"jsonata","value":"(\r\n    /***********************************************************************\\\r\n    * Use the CDISC Pilot data as a start point                             *\r\n    \\***********************************************************************/\r\n\r\n    $old_study := $loadFile(\"./testdata/CDISC_Pilot_Study.json\");\r\n    $old_svid := $old_study.study.versions[0].id;\r\n\r\n    /***********************************************************************\\\r\n    * Create and insert a copy of the first study design and make the study *\r\n    * role apply to the study version (all study designs).                  *\r\n    \\***********************************************************************/\r\n    $maxids := $utils.get_maxids($old_study);\r\n    $idmap := $utils.get_idmap($old_study.study.versions[0].studyDesigns[0],$maxids);\r\n    $new_sd1 := $utils.make_dup($old_study.study.versions[0].studyDesigns[0],$idmap)\r\n                ~> |$|{\"name\": \"Study Design 2\", \"description\": \"A secondary design for the study\"}|;\r\n    $new_study1a := $old_study\r\n                ~> |study.versions[0]|{\"studyDesigns\": $append(studyDesigns,$new_sd1)}|\r\n                ~> |study.versions[0].roles[0]|{\"appliesToIds\": [$old_svid]}|;\r\n\r\n    /***********************************************************************\\\r\n    * Create and insert another copy of the first study design              *\r\n    \\***********************************************************************/\r\n    $maxids := $utils.get_maxids($new_study1a);\r\n    $idmap := $utils.get_idmap($new_study1a.study.versions[0].studyDesigns[0],$maxids);\r\n    $new_sd2 := $utils.make_dup($new_study1a.study.versions[0].studyDesigns[0],$idmap)\r\n                ~> |$|{\"name\": \"Study Design 3\", \"description\": \"A tertiary design for the study\"}|;\r\n    $new_study1b := $new_study1a\r\n                ~> |study.versions[0]|{\"studyDesigns\": $append(studyDesigns,$new_sd2)}|;\r\n\r\n    /***********************************************************************\\\r\n    * Create and insert a copy of the first role, changing to study subject *\r\n    \\***********************************************************************/\r\n    $maxids := $utils.get_maxids($new_study1b);\r\n    $idmap := $utils.get_idmap($new_study1b.study.versions[0].roles[0],$maxids);\r\n    $new_role1 := $utils.make_dup($new_study1b.study.versions[0].roles[0],$idmap)\r\n                ~> |$|{\"name\": \"ROLE_2\", \"label\": \"Subject\"}|\r\n                ~> |code|{\"code\": \"C41189\", \"decode\": \"Study Subject\"}|;\r\n    $new_study2 := $new_study1b ~> |study.versions[0]|{\"roles\": $append(roles,$new_role1)}|;\r\n\r\n    /***********************************************************************\\\r\n    * Create and insert a copy of the first role, changing to sponsor       *\r\n    \\***********************************************************************/\r\n    $maxids := $utils.get_maxids($new_study2);\r\n    $idmap := $utils.get_idmap($new_study2.study.versions[0].roles[0],$maxids);\r\n    $new_role2 := $utils.make_dup($new_study2.study.versions[0].roles[0],$idmap)\r\n                ~> |$|{\"name\": \"ROLE_3\", \"label\": \"Investigator\"}|\r\n                ~> |code|{\"code\": \"C25936\", \"decode\": \"Investigator\"}|;\r\n    $new_study3 := $new_study2 ~> |study.versions[0]|{\"roles\": $append(roles,$new_role2)}|;\r\n\r\n    /***********************************************************************\\\r\n    * Write out as positive unit test data.                                 *\r\n    \\***********************************************************************/\r\n    $writeFile(\"./rules/DDF00192/unit-test-coreid-DDF00192-positive-1.json\",$new_study3);\r\n    \r\n    /***********************************************************************\\\r\n    * Make the following updates then write out as negative unit test data: *\r\n    * - Make the first apply only to the second study design.               *\r\n    * - Make the second role apply to an invalid id.                        *\r\n    * - Make the first third role apply only to the second and third        *\r\n    *   study designs and remove its masking.                               *\r\n    *                                                                       *\r\n    * As a result:                                                          *\r\n    * - The first study design will have no applicable roles                *\r\n    * - The second study design will have two applicable roles but only one *\r\n    *   of them will be masked.                                             *\r\n    * - The third study design only has one applicable role and it will not *\r\n    *   be masked.                                                          *\r\n    \\***********************************************************************/\r\n    $new_study4 := $new_study3\r\n                    ~> |study.versions[0].roles[0]|{\"appliesToIds\": [$new_sd1.id]}|\r\n                    ~> |study.versions[0].roles[1]|{\"appliesToIds\": [\"StudyVersion_xx\"]}|\r\n                    ~> |study.versions[0].roles[2]|{\"appliesToIds\": [$new_sd1.id, $new_sd2.id],\"masking\": null}|;\r\n    $writeFile(\"./rules/DDF00192/unit-test-coreid-DDF00192-negative-1.json\",$new_study4);\r\n)","outputs":[]},{"kind":1,"language":"markdown","value":"Test positive data","outputs":[]},{"kind":2,"language":"jsonata","value":"(\r\n    $pos_data := $loadFile(\"./rules/DDF00192/unit-test-coreid-DDF00192-positive-1.json\");\r\n    $utils.report($rule,$pos_data)\r\n)","outputs":[]},{"kind":1,"language":"markdown","value":"Test negative data","outputs":[]},{"kind":2,"language":"jsonata","value":"(\r\n    $neg_data := $loadFile(\"./rules/DDF00192/unit-test-coreid-DDF00192-negative-1.json\");\r\n    $utils.report($rule,$neg_data)\r\n)","outputs":[]}]