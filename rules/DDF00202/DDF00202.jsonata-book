[{"kind":1,"language":"markdown","value":"Import utility functions","outputs":[]},{"kind":2,"language":"jsonata","value":"$utils := $merge([$import(\"./utils/get_idmap.jsonata\")\r\n                 ,$import(\"./utils/get_maxids.jsonata\")\r\n                 ,$import(\"./utils/make_dup.jsonata\")\r\n                 ,$import(\"./utils/get_rule.jsonata\")\r\n                 ,$import(\"./utils/report.jsonata\")]) ","outputs":[]},{"kind":1,"language":"markdown","value":"Define test","outputs":[]},{"kind":2,"language":"jsonata","value":"$rule := $utils.get_rule($import(\"./rules/DDF00202/DDF00202.json\"))","outputs":[]},{"kind":1,"language":"markdown","value":"Create test data","outputs":[]},{"kind":2,"language":"jsonata","value":"(\r\n    /***********************************************************************\\\r\n    * Use the CDISC Pilot data as a start point                             *\r\n    \\***********************************************************************/\r\n\r\n    $old_study := $loadFile(\"./testdata/CDISC_Pilot_Study.json\");\r\n    $old_svid := $old_study.study.versions[0].id;\r\n    $old_sdid := $old_study.study.versions[0].studyDesigns[0].id;\r\n\r\n    /***********************************************************************\\\r\n    * Create and insert a copy of the first role, changing to study subject *\r\n    \\***********************************************************************/\r\n    $maxids := $utils.get_maxids($old_study);\r\n    $idmap := $utils.get_idmap($old_study.study.versions[0].roles[0],$maxids);\r\n    $new_role1 := $utils.make_dup($old_study.study.versions[0].roles[0],$idmap)\r\n                ~> |$|{\"name\": \"ROLE_2\", \"label\": \"Subject\", \"appliesToIds\": [$old_sdid,$new_sd1.id]}|\r\n                ~> |code|{\"code\": \"C41189\", \"decode\": \"Study Subject\"}|;\r\n    $new_study2 := $old_study ~> |study.versions[0]|{\"roles\": $append(roles,$new_role1)}|;\r\n\r\n    /***********************************************************************\\\r\n    * Create and insert a copy of the first role, changing to sponsor       *\r\n    \\***********************************************************************/\r\n    $maxids := $utils.get_maxids($new_study2);\r\n    $idmap := $utils.get_idmap($new_study2.study.versions[0].roles[0],$maxids);\r\n    $new_role2 := $utils.make_dup($new_study2.study.versions[0].roles[0],$idmap)\r\n                ~> |$|{\"name\": \"ROLE_3\", \"label\": \"Investigator\", \"appliesToIds\": [$new_sd2.id]}|\r\n                ~> |code|{\"code\": \"C25936\", \"decode\": \"Investigator\"}|;\r\n    $new_study3 := $new_study2 ~> |study.versions[0]|{\"roles\": $append(roles,$new_role2)}|;\r\n\r\n    /***********************************************************************\\\r\n    * Write out as positive unit test data.                                 *\r\n    \\***********************************************************************/\r\n    $writeFile(\"./rules/DDF00202/unit-test-coreid-DDF00202-positive-1.json\",$new_study3);\r\n    \r\n    /***********************************************************************\\\r\n    * Make the following updates then write out as negative unit test data: *\r\n    * - Remove the organizationIds the first role (which is a sponsor role) *\r\n    *   so the sponsor role has no organizations.                           *\r\n    \\***********************************************************************/\r\n    $new_study4 := $new_study3\r\n                    ~> |study.versions[0].roles[0]|{\"organizationIds\": []}|;\r\n    $writeFile(\"./rules/DDF00202/unit-test-coreid-DDF00202-negative-1.json\",$new_study4);\r\n\r\n    /***********************************************************************\\\r\n    * Make the following updates then write out as negative unit test data: *\r\n    * - Add the id of the third organization to organizationIds of the      *\r\n    *   first study role (which is a sponsor role) so the sponsor role has  *\r\n    *   two organizations.                                                  *\r\n    \\***********************************************************************/\r\n    $org3id := $new_study3.study.versions[0].organizations[2].id;\r\n    $new_study5 := $new_study3\r\n                    ~> |study.versions[0].roles[0]|{\"organizationIds\": $append(organizationIds,$org3id)}|;\r\n    $writeFile(\"./rules/DDF00202/unit-test-coreid-DDF00202-negative-2.json\",$new_study5);\r\n    /***********************************************************************\\\r\n    * Make the following updates then write out as negative unit test data: *\r\n    * - Set organizationIds of the first study role (which is a sponsor     *\r\n    *   role) to be an invalid organization id so the sponsor role has no   *\r\n    *   organizations.                                                      *\r\n    \\***********************************************************************/\r\n    $new_study6 := $new_study3\r\n                    ~> |study.versions[0].roles[0]|{\"organizationIds\": [\"Organization_xxx\"]}|;\r\n    $writeFile(\"./rules/DDF00202/unit-test-coreid-DDF00202-negative-3.json\",$new_study6);\r\n    /***********************************************************************\\\r\n    * Make the following updates then write out as negative unit test data: *\r\n    * - Add an invalid id to organizationIds of the first study role (which *\r\n    *   is a sponsor role) so the sponsor role has two organizationIds.     *\r\n    \\***********************************************************************/\r\n    $new_study7 := $new_study3\r\n                    ~> |study.versions[0].roles[0]|{\"organizationIds\": $append(organizationIds,\"Organization_xxx\")}|;\r\n    $writeFile(\"./rules/DDF00202/unit-test-coreid-DDF00202-negative-4.json\",$new_study7);\r\n)","outputs":[]},{"kind":1,"language":"markdown","value":"Test positive data","outputs":[]},{"kind":2,"language":"jsonata","value":"(\r\n    $pos_data := $loadFile(\"./rules/DDF00202/unit-test-coreid-DDF00202-positive-1.json\");\r\n    $utils.report($rule,$pos_data)\r\n)","outputs":[]},{"kind":1,"language":"markdown","value":"Test negative data - sponsor role with no organizations","outputs":[]},{"kind":2,"language":"jsonata","value":"(\r\n    $neg_data := $loadFile(\"./rules/DDF00202/unit-test-coreid-DDF00202-negative-1.json\");\r\n    $utils.report($rule,$neg_data)\r\n)","outputs":[]},{"kind":1,"language":"markdown","value":"Test negative data - sponsor role with 2 valid organizations","outputs":[]},{"kind":2,"language":"jsonata","value":"(\r\n    $neg_data := $loadFile(\"./rules/DDF00202/unit-test-coreid-DDF00202-negative-2.json\");\r\n    $utils.report($rule,$neg_data)\r\n)","outputs":[]},{"kind":1,"language":"markdown","value":"Test negative data - sponsor role with 0 organizations: one invalid organizationId","outputs":[]},{"kind":2,"language":"jsonata","value":"(\r\n    $neg_data := $loadFile(\"./rules/DDF00202/unit-test-coreid-DDF00202-negative-3.json\");\r\n    $utils.report($rule,$neg_data)\r\n)","outputs":[]},{"kind":1,"language":"markdown","value":"Test negative data - sponsor role with 1 organization: one valid organizationId and one invalid organizationId","outputs":[]},{"kind":2,"language":"jsonata","value":"(\r\n    $neg_data := $loadFile(\"./rules/DDF00202/unit-test-coreid-DDF00202-negative-4.json\");\r\n    $utils.report($rule,$neg_data)\r\n)","outputs":[]}]